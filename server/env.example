PORT=5000
NODE_ENV=development

# MongoDB connection string
# Local: mongodb://localhost:27017/library-manager
# MongoDB Atlas: mongodb+srv://username:password@cluster.mongodb.net/library-manager
MONGODB_URI=mongodb://localhost:27017/library-manager

# Set to 'true' to seed sample/mock data when initializing the database
# Default: false (clean database without mock data)
SEED_SAMPLE=false
